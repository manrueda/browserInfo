{
  "name": "browser-info",
  "version": "0.0.4",
  "description": "Lightweight bower package to extract browser and OS info",
  "main": "dist/browserInfo.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/devVersion.js tape test/distVersion.js",
    "testDist": "tape test/distVersion.js",
    "cov": "istanbul cover node_modules/.bin/tape --report lcovonly test/devVersion.js && coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ManRueda/browserInfo.git"
  },
  "author": "Manuel Rueda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ManRueda/browserInfo/issues"
  },
  "homepage": "https://github.com/ManRueda/browserInfo#readme",
  "devDependencies": {
    "coveralls": "2.11.4",
    "grunt": "0.4.5",
    "grunt-contrib-jshint": "0.11.3",
    "grunt-contrib-uglify": "0.9.2",
    "istanbul": "0.3.22",
    "tape": "4.2.1"
  }
}
